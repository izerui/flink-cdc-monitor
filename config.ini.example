[mysql]
host = localhost
port = 3306
databases = db1,db2,db3
username = root
password = root
ignored_table_prefixes = tmp_,temp_

[postgresql]
host = localhost
port = 5432
database = postgres
username = postgres
password = postgres

[monitor]
# 刷新间隔，单位秒
refresh_interval = 2
# 是否启用清屏
enable_clear_screen = true
# MySQL更新间隔，单位秒
mysql_update_interval = 5
# 每页显示的记录数
page_size = 50
# 自动翻页间隔（秒）
page_interval = 20

